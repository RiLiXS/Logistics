<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">





    <View
        android:id="@+id/view8"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <View
        android:id="@+id/view9"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <TextView
        android:visibility="gone"
        android:id="@+id/tv_num"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="2dp"
        android:maxLines="1"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/tv_name"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="2dp"
        android:lines="3"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/tv_num"
        app:layout_constraintRight_toLeftOf="@+id/tv_time"
        app:layout_constraintTop_toTopOf="@+id/tv_num" />

    <TextView
        android:id="@+id/tv_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="2dp"
        android:lines="3"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/tv_name"
        app:layout_constraintRight_toLeftOf="@+id/tv_people"
        app:layout_constraintTop_toTopOf="@+id/tv_name" />

    <TextView
        android:id="@+id/tv_people"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="2dp"
        android:lines="3"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/tv_time"
        app:layout_constraintRight_toLeftOf="@+id/tv_qualifiedNum"
        app:layout_constraintTop_toTopOf="@+id/tv_name" />

    <TextView
        android:id="@+id/tv_qualifiedNum"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="2dp"
        android:lines="3"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintLeft_toRightOf="@+id/tv_people"
        app:layout_constraintRight_toLeftOf="@+id/tv_unqualifiedNum"
        app:layout_constraintTop_toTopOf="@+id/tv_name" />

    <TextView
        android:id="@+id/tv_unqualifiedNum"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="2dp"
        android:lines="3"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintRight_toLeftOf="@+id/tv_state"
        app:layout_constraintLeft_toRightOf="@+id/tv_qualifiedNum"
        app:layout_constraintTop_toTopOf="@+id/tv_name" />

    <TextView
        android:id="@+id/tv_state"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="2dp"
        android:lines="3"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/tv_unqualifiedNum"
        app:layout_constraintTop_toTopOf="@+id/tv_name" />

    <View
        android:id="@+id/view10"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/tv_unqualifiedNum" />

    <View
        android:id="@+id/view11"
        android:visibility="gone"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_num"
        app:layout_constraintLeft_toRightOf="@+id/tv_num"
        app:layout_constraintTop_toTopOf="@+id/tv_num" />

    <View
        android:id="@+id/view12"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_name"
        app:layout_constraintLeft_toRightOf="@+id/tv_name"
        app:layout_constraintTop_toTopOf="@+id/tv_name" />

    <View
        android:id="@+id/view13"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_name"
        app:layout_constraintLeft_toRightOf="@+id/tv_time"
        app:layout_constraintTop_toTopOf="@+id/tv_name" />

    <View
        android:id="@+id/view14"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_name"
        app:layout_constraintLeft_toRightOf="@+id/tv_people"
        app:layout_constraintTop_toTopOf="@+id/tv_name" />

    <View
        android:id="@+id/view15"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_name"
        app:layout_constraintLeft_toRightOf="@+id/tv_qualifiedNum"
        app:layout_constraintTop_toTopOf="@+id/tv_name" />

    <View
        android:id="@+id/view16"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_name"
        app:layout_constraintLeft_toRightOf="@+id/tv_unqualifiedNum"
        app:layout_constraintTop_toTopOf="@+id/tv_name" />


    <TextView
        android:id="@+id/title_total_num"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="2dp"
        android:text="合计"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintRight_toRightOf="@+id/tv_time"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view10" />
    <TextView
        android:id="@+id/tv_total_plan_num"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="2dp"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintRight_toRightOf="@+id/tv_people"
        app:layout_constraintLeft_toLeftOf="@+id/tv_people"
        app:layout_constraintTop_toBottomOf="@+id/view10" />
    <TextView
        android:id="@+id/tv_total_task_num"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="2dp"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintRight_toRightOf="@+id/tv_qualifiedNum"
        app:layout_constraintLeft_toLeftOf="@+id/tv_qualifiedNum"
        app:layout_constraintTop_toBottomOf="@+id/view10" />



    <View
        android:id="@+id/view20"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/title_total_num"
        />

    <View
        android:id="@+id/view17"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/title_total_num"
        app:layout_constraintLeft_toRightOf="@+id/title_total_num"
        app:layout_constraintTop_toTopOf="@+id/title_total_num" />
    <View
        android:id="@+id/view18"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_total_plan_num"
        app:layout_constraintLeft_toRightOf="@+id/tv_total_plan_num"
        app:layout_constraintTop_toTopOf="@+id/tv_total_plan_num" />
    <View
        android:id="@+id/view19"
        android:layout_width="0.5dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/tv_total_task_num"
        app:layout_constraintLeft_toRightOf="@+id/tv_total_task_num"
        app:layout_constraintTop_toTopOf="@+id/tv_total_task_num" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/item_group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="visible"
        app:constraint_referenced_ids="view20,view17,view18,view19,tv_total_task_num,
        tv_total_plan_num,title_total_num
" />

</androidx.constraintlayout.widget.ConstraintLayout>